{
  "name": "DoraCodeLens JSON Utilities Demo",
  "version": "1.0.0",
  "features": {
    "formatting": {
      "beautification": true,
      "validation": true,
      "errorReporting": true,
      "syntaxHighlighting": true
    },
    "treeView": {
      "expandableNodes": true,
      "searchFunctionality": true,
      "exportOptions": [
        "json",
        "csv",
        "txt"
      ],
      "metadata": {
        "nodeCount": true,
        "maxDepth": true,
        "typeStatistics": true
      }
    }
  },
  "supportedTypes": [
    "object",
    "array",
    "string",
    "number",
    "boolean",
    "null"
  ],
  "configuration": {
    "indent": 2,
    "sortKeys": false,
    "removeComments": true,
    "removeTrailingCommas": true,
    "insertFinalNewline": true
  },
  "examples": [
    {
      "type": "simple",
      "data": {
        "key": "value"
      }
    },
    {
      "type": "nested",
      "data": {
        "level1": {
          "level2": {
            "level3": [
              "item1",
              "item2",
              "item3"
            ]
          }
        }
      }
    },
    {
      "type": "array",
      "data": [
        1,
        2,
        3,
        {
          "nested": true
        }
      ]
    }
  ],
  "testData": {
    "nullValue": null,
    "emptyString": "",
    "zeroValue": 0,
    "falseValue": false,
    "largeNumber": 1234567890,
    "specialChars": "Special chars: !@#$%^&*()_+-=[]{}|;':\",./<>?"
  }
}
