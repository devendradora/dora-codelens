{
  "name": "doracodelens",
  "scripts": {
    "compile": "tsc -p .",
    "test": "npm run compile && node ./out/test/runTest.js",
    "test:unit": "npm run compile && mocha out/test/**/*.test.js --timeout 10000",
    "test:integration": "npm run compile && mocha out/test/code-lens-integration.test.js --timeout 15000",
    "test:performance": "npm run compile && mocha out/test/performance.test.js --timeout 20000",
    "test:e2e": "npm run compile && mocha out/test/end-to-end.test.js --timeout 30000",
    "pretest": "npm run compile"
  },
  "displayName": "DoraCodeLens",
  "description": "Advanced VS Code extension for comprehensive Python project analysis with Git analytics, enhanced visualizations, JSON utilities, and database schema analysis",
  "version": "0.1.1",
  "publisher": "DevendraDora",
  "icon": "resources/dora-code-lens-kiro.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/devendradora/code-mind-map.git"
  },
  "homepage": "https://github.com/devendradora/code-mind-map#readme",
  "bugs": {
    "url": "https://github.com/devendradora/code-mind-map/issues"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "python",
    "visualization",
    "graph",
    "complexity",
    "analysis",
    "git-analytics",
    "database-schema",
    "json-utilities"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:json",
    "workspaceContains:**/*.py",
    "workspaceContains:requirements.txt",
    "workspaceContains:pyproject.toml",
    "workspaceContains:Pipfile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "DoraCodeLens",
      "properties": {
        "doracodelens.pythonPath": {
          "type": "string",
          "default": "python3",
          "description": "Path to Python executable (python3, python, or full path like /usr/bin/python3)",
          "scope": "resource"
        },
        "doracodelens.analysisTimeout": {
          "type": "number",
          "default": 120,
          "description": "Timeout for analysis operations in seconds",
          "minimum": 30,
          "maximum": 600,
          "scope": "resource"
        },
        "doracodelens.enableDebugLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug logging for troubleshooting",
          "scope": "resource"
        },
        "doracodelens.codeLens.complexityThresholds.low": {
          "type": "number",
          "default": 5,
          "description": "Threshold for low complexity (green indicator)",
          "minimum": 1,
          "maximum": 50,
          "scope": "resource"
        },
        "doracodelens.codeLens.complexityThresholds.medium": {
          "type": "number",
          "default": 10,
          "description": "Threshold for medium complexity (yellow indicator)",
          "minimum": 1,
          "maximum": 50,
          "scope": "resource"
        },
        "doracodelens.codeLens.complexityThresholds.high": {
          "type": "number",
          "default": 11,
          "description": "Threshold for high complexity (red indicator)",
          "minimum": 1,
          "maximum": 50,
          "scope": "resource"
        },
        "doracodelens.codeLens.showComplexity": {
          "type": "boolean",
          "default": true,
          "description": "Show complexity indicators in code lens",
          "scope": "resource"
        },
        "doracodelens.codeLens.showSuggestions": {
          "type": "boolean",
          "default": true,
          "description": "Show actionable suggestions in code lens",
          "scope": "resource"
        },
        "doracodelens.codeLens.maxSuggestionsPerFunction": {
          "type": "number",
          "default": 3,
          "description": "Maximum number of suggestions to show per function",
          "minimum": 1,
          "maximum": 10,
          "scope": "resource"
        },
        "doracodelens.guidance.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable code lens activation guidance",
          "scope": "resource"
        },
        "doracodelens.guidance.preferredAnalysisType": {
          "type": "string",
          "enum": ["current-file", "full-project", "ask-each-time"],
          "default": "ask-each-time",
          "description": "Default analysis type when enabling code lens",
          "scope": "resource"
        },
        "doracodelens.guidance.autoRunAnalysisOnEnable": {
          "type": "boolean",
          "default": false,
          "description": "Automatically run analysis when code lens is enabled",
          "scope": "resource"
        },
        "doracodelens.guidance.showWelcomeMessage": {
          "type": "boolean",
          "default": true,
          "description": "Show welcome message for first-time users",
          "scope": "resource"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "doracodelens-explorer",
          "title": "DoraCodeLens",
          "icon": "resources/light/doracodelens.svg"
        }
      ]
    },
    "submenus": [
      {
        "id": "doracodelens.contextMenu",
        "label": "DoraCodeLens"
      }
    ],
    "views": {
      "doracodelens-explorer": [
        {
          "id": "doracodelens-sidebar",
          "name": "Project Analysis",
          "icon": "resources/light/doracodelens.svg",
          "contextualTitle": "Project Analysis"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "submenu": "doracodelens.contextMenu",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "submenu": "doracodelens.contextMenu",
          "group": "navigation"
        }
      ],
      "doracodelens.contextMenu": [
        {
          "command": "doracodelens.analyzeFullCode",
          "group": "analyze@1",
          "when": "resourceLangId == python"
        },
        {
          "command": "doracodelens.analyzeCurrentFile",
          "group": "analyze@2",
          "when": "resourceLangId == python"
        },
        {
          "command": "doracodelens.enableCodeLens",
          "group": "analyze@3",
          "when": "resourceLangId == python && !doracodelens.codeLensEnabled"
        },
        {
          "command": "doracodelens.disableCodeLens",
          "group": "analyze@3",
          "when": "resourceLangId == python && doracodelens.codeLensEnabled"
        },
        {
          "command": "doracodelens.analyzeDatabaseSchema",
          "group": "database@1",
          "when": "resourceLangId == python"
        },
        {
          "command": "doracodelens.analyzeGitAnalytics",
          "group": "git@1"
        },
        {
          "command": "doracodelens.jsonFormat",
          "group": "json@1",
          "when": "doracodelens.jsonContext"
        },
        {
          "command": "doracodelens.jsonTreeView",
          "group": "json@2",
          "when": "doracodelens.jsonContext"
        },
        {
          "command": "doracodelens.jsonFix",
          "group": "json@3",
          "when": "doracodelens.jsonContext"
        },
        {
          "command": "doracodelens.jsonMinify",
          "group": "json@4",
          "when": "doracodelens.jsonContext"
        },
        {
          "command": "doracodelens.setupPythonPath",
          "group": "setup@1"
        },
        {
          "command": "doracodelens.detectPythonPath",
          "group": "setup@2"
        },
        {
          "command": "doracodelens.openSettings",
          "group": "setup@3"
        }
      ],
      "editor/title": [
        {
          "command": "doracodelens.enableCodeLens",
          "when": "resourceLangId == python && !doracodelens.codeLensEnabled",
          "group": "navigation@1"
        },
        {
          "command": "doracodelens.disableCodeLens",
          "when": "resourceLangId == python && doracodelens.codeLensEnabled",
          "group": "navigation@1"
        }
      ],
      "view/title": [
        {
          "command": "doracodelens.refreshSidebar",
          "when": "view == doracodelens-sidebar",
          "group": "navigation"
        },
        {
          "command": "doracodelens.openSettings",
          "when": "view == doracodelens-sidebar",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "doracodelens.showDependencies",
          "when": "view == doracodelens-sidebar && viewItem == module",
          "group": "inline"
        },
        {
          "command": "doracodelens.showFunctionComplexityDetails",
          "when": "view == doracodelens-sidebar && viewItem == function",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "doracodelens.analyzeFullCode",
        "title": "Full Code Analysis",
        "category": "DoraCodeLens",
        "icon": "$(graph)"
      },
      {
        "command": "doracodelens.refreshFullCodeAnalysis",
        "title": "Refresh Full Code Analysis",
        "category": "DoraCodeLens",
        "icon": "$(refresh)"
      },
      {
        "command": "doracodelens.analyzeCurrentFile",
        "title": "Current File Analysis",
        "category": "DoraCodeLens",
        "icon": "$(file-code)"
      },
      {
        "command": "doracodelens.analyzeDatabaseSchema",
        "title": "Database Schema Analysis",
        "category": "DoraCodeLens",
        "icon": "$(database)"
      },
      {
        "command": "doracodelens.analyzeGitAnalytics",
        "title": "Git Analytics",
        "category": "DoraCodeLens",
        "icon": "$(git-compare)"
      },
      {
        "command": "doracodelens.jsonFormat",
        "title": "JSON Format",
        "category": "DoraCodeLens",
        "icon": "$(json)"
      },
      {
        "command": "doracodelens.jsonTreeView",
        "title": "JSON Tree View",
        "category": "DoraCodeLens",
        "icon": "$(list-tree)"
      },
      {
        "command": "doracodelens.jsonFix",
        "title": "JSON Fix (Python Dict)",
        "category": "DoraCodeLens",
        "icon": "$(wrench)"
      },
      {
        "command": "doracodelens.jsonMinify",
        "title": "JSON Minify",
        "category": "DoraCodeLens",
        "icon": "$(fold)"
      },
      {
        "command": "doracodelens.refreshSidebar",
        "title": "Refresh",
        "category": "DoraCodeLens",
        "icon": "$(refresh)"
      },
      {
        "command": "doracodelens.openSettings",
        "title": "Settings",
        "category": "DoraCodeLens",
        "icon": "$(settings-gear)"
      },
      {
        "command": "doracodelens.clearCache",
        "title": "Clear Cache",
        "category": "DoraCodeLens",
        "icon": "$(clear-all)"
      },
      {
        "command": "doracodelens.cancelAnalysis",
        "title": "Cancel Analysis",
        "category": "DoraCodeLens",
        "icon": "$(stop)"
      },
      {
        "command": "doracodelens.navigateToItem",
        "title": "Navigate to Item",
        "category": "DoraCodeLens"
      },
      {
        "command": "doracodelens.filterSidebar",
        "title": "Filter",
        "category": "DoraCodeLens",
        "icon": "$(filter)"
      },
      {
        "command": "doracodelens.selectModule",
        "title": "Select Module",
        "category": "DoraCodeLens"
      },
      {
        "command": "doracodelens.clearSelection",
        "title": "Clear Selection",
        "category": "DoraCodeLens",
        "icon": "$(clear-all)"
      },
      {
        "command": "doracodelens.showDependencies",
        "title": "Show Dependencies",
        "category": "DoraCodeLens",
        "icon": "$(references)"
      },
      {
        "command": "doracodelens.showFunctionComplexityDetails",
        "title": "Show Function Complexity Details",
        "category": "DoraCodeLens"
      },
      {
        "command": "doracodelens.showGraphView",
        "title": "Graph View",
        "category": "DoraCodeLens",
        "icon": "$(type-hierarchy)"
      },
      {
        "command": "doracodelens.enableCodeLens",
        "title": "Code Lens (On)",
        "category": "DoraCodeLens",
        "icon": "$(eye)"
      },
      {
        "command": "doracodelens.disableCodeLens",
        "title": "Code Lens (Off)",
        "category": "DoraCodeLens",
        "icon": "$(eye-closed)"
      },
      {
        "command": "doracodelens.applySuggestion",
        "title": "Apply Code Lens Suggestion",
        "category": "DoraCodeLens",
        "icon": "$(lightbulb)"
      },
      {
        "command": "doracodelens.showSuggestionDetails",
        "title": "Show Code Lens Suggestion Details",
        "category": "DoraCodeLens",
        "icon": "$(info)"
      },
      {
        "command": "doracodelens.guidance.showWelcome",
        "title": "Show Welcome Guidance",
        "category": "DoraCodeLens",
        "icon": "$(star)"
      },
      {
        "command": "doracodelens.guidance.analyzeCurrentFile",
        "title": "Analyze Current File (Guidance)",
        "category": "DoraCodeLens",
        "icon": "$(file-code)"
      },
      {
        "command": "doracodelens.guidance.analyzeFullProject",
        "title": "Analyze Full Project (Guidance)",
        "category": "DoraCodeLens",
        "icon": "$(project)"
      },
      {
        "command": "doracodelens.guidance.refreshAnalysis",
        "title": "Refresh Analysis (Guidance)",
        "category": "DoraCodeLens",
        "icon": "$(refresh)"
      },
      {
        "command": "doracodelens.guidance.retryAnalysis",
        "title": "Retry Analysis (Guidance)",
        "category": "DoraCodeLens",
        "icon": "$(debug-restart)"
      },
      {
        "command": "doracodelens.guidance.showProgress",
        "title": "Show Analysis Progress",
        "category": "DoraCodeLens",
        "icon": "$(loading)"
      },
      {
        "command": "doracodelens.guidance.changePreferences",
        "title": "Change Guidance Preferences",
        "category": "DoraCodeLens",
        "icon": "$(gear)"
      },
      {
        "command": "doracodelens.guidance.setPreferredAnalysis",
        "title": "Set Preferred Analysis Type",
        "category": "DoraCodeLens",
        "icon": "$(settings)"
      },
      {
        "command": "doracodelens.guidance.showErrorDetails",
        "title": "Show Error Details",
        "category": "DoraCodeLens",
        "icon": "$(info)"
      },
      {
        "command": "doracodelens.setupPythonPath",
        "title": "Setup Python Path",
        "category": "DoraCodeLens",
        "icon": "$(settings-gear)"
      },
      {
        "command": "doracodelens.detectPythonPath",
        "title": "Auto-Detect Python Path",
        "category": "DoraCodeLens",
        "icon": "$(search)"
      }
    ],
    "scripts": {
      "compile": "tsc -p ."
    }
  }
}
